import SiteFooter from "@/components/SiteFooter.astro"
import SiteHeader from "@/components/SiteHeader.astro"
import "@/styles/main.scss"
const { title } = Astro.props
---

<!doctype html>
<html lang="en">
	<BaseHead {title} />
	<body>
		<div class="app--container">
			<SiteHeader />
			<main id="main">
				<slot />
			</main>
			<SiteFooter />
		</div>
	</body>
</html>
